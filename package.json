{
  "name": "typescript-graphql-test",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "del-cli --force ./build",
    "build": "tsc",
    "prestart": "yarn build",
    "start": "node -r dotenv/config build/app.js",
    "start:dev": "ts-node-dev --transpile-only -r dotenv/config src/app.ts",
    "test": "jest --projects ./tests/integration --runInBand",
    "db:migrate": "ts-node typeorm migrations:generate -n"
  },
  "dependencies": {
    "apollo-server-express": "^3.12.0",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "15.3.0",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "^3.3.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.5",
    "del-cli": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "typescript",
    "graphql"
  ]
}
